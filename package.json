{
  "name": "board-games",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbazurin/board-games.git"
  },
  "license": "MIT",
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start.server": "nodemon --config nodemon/nodemon-dev.json",
    "start.server:debug": "nodemon --config nodemon/nodemon-debug.json",
    "start.server:prod": "nodemon --config nodemon/nodemon-prod.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "clean": "rimraf dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-translations": "ngx-translate-extract -i ./src/client/ -o ./src/client/assets/i18n/en.json ./src/client/assets/i18n/ru.json ./src/client/assets/i18n/ua.json -c -s -f namespaced-json -m markForTranslation",
    "heroku-postbuild": "npm run clean && npm run lint && npm run build:prod"
  },
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@nestjs/common": "^5.6.2",
    "@nestjs/core": "^5.6.2",
    "@nestjs/websockets": "^5.6.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angulartics2": "^7.4.1",
    "core-js": "^2.6.3",
    "hammerjs": "^2.0.8",
    "js-sha256": "^0.9.0",
    "nodemon": "^1.18.9",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.5.2",
    "rxjs": "~6.3.3",
    "typescript": "~3.1.6",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.19",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "~5.11.0"
  }
}
